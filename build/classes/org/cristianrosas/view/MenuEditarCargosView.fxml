<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.cristianrosas.controller.MenuEditarCargosController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/FondoFormCargos.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfCargoId" editable="false" layoutX="31.0" layoutY="154.0" prefHeight="34.0" prefWidth="182.0" promptText="cargo Id" />
      <TextField fx:id="tfNombreCargo" layoutX="31.0" layoutY="277.0" prefHeight="34.0" prefWidth="230.0" />
      <TextArea fx:id="taDescripcion" layoutX="75.0" layoutY="484.0" prefHeight="161.0" prefWidth="256.0" wrapText="true" />
      <Button fx:id="btnAgregar" layoutX="395.0" layoutY="328.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="60.0" prefWidth="90.0" style="-fx-background-color: green;" text="Agregar" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btnCancelar" layoutX="395.0" layoutY="408.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="60.0" prefWidth="90.0" style="-fx-background-color: red;" text="Cancelar" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
